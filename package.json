{
  "name": "devenv",
  "version": "0.0.1",
  "author": "Kamil Kashaev <kashaev@gmail.com>",
  "description": "Small front-end environment",
  "scripts": {
    "start": "npm run dev",
    "server": "concurrently \"clear\" \"npm run srv\" \"npm run proxy2\"",
    "srv": "http-server build -p 7777 -s -c10",
    "proxy": "lt -p 7777 -s psell",
    "proxy2": " ",
    "dev": "NODE_ENV='development' concurrently \"npm run webpack\" \"npm run sass\"",
    "devwin": "NODE_ENV='development' concurrently 'npm run webpack' 'npm run sass'",
    "dev2": "NODE_ENV='development' concurrently \"npm run webpack\" \"npm run vendors\" \"npm run sass\"",
    "prod": "NODE_ENV='production' concurrently \"npm run webpack\" \"npm run vendors\" \"npm run sass\"",
    "webpack": "babel-node tools/tasks/webpack",
    "vendors": "babel-node tools/tasks/vendors",
    "sass": "babel-node tools/tasks/sass"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.1.4",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "clean-css": "^4.1.7",
    "concurrently": "^3.5.0",
    "cssnano": "^3.10.0",
    "fancy-log": "^1.3.0",
    "fastclick": "^1.0.6",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "mobile-detect": "^1.3.6",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.9",
    "postcss-reporter": "^3.0.0",
    "pretty-hrtime": "1.0.2",
    "stylelint": "^7.10.1",
    "swiper": "^3.4.2",
    "three-orbit-controls": "^82.1.0",
    "tiny-lr": "^1.0.5",
    "uglifyjs-webpack-plugin": "^0.4.4",
    "webpack": "2.5.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bourbon": "^5.0.0",
    "dom-create-element": "^1.0.2",
    "whatwg-fetch": "^2.0.3"
  }
}
